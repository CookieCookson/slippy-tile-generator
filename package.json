{
  "name": "slippy-tile-generator",
  "version": "1.0.0",
  "description": "A node.js slippy map tile generator.",
  "author": {
    "name": "Christian Cook",
    "url": "https://www.cookie.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CookieCookson/slippy-tile-generator.git"
  },
  "bugs": {
      "url": "https://github.com/CookieCookson/slippy-tile-generator/issues"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "bin": {
    "slippy": "./dist/index.js"
  },
  "license": "ISC",
  "keywords": [
    "slippy",
    "map",
    "tile",
    "tiles",
    "generator",
    "leaflet",
    "mapbox",
    "google"
  ],
  "dependencies": {
    "commander": "^11.0.0",
    "jimp": "^0.22.10",
    "progress": "^2.0.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.7",
    "@types/progress": "^2.0.5",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "rootDir": "src",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  }
